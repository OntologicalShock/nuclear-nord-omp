{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "alma": "#0069da",
    "alpine": "#0d597f",
    "arch-bl": "#1793d1",
    "arch-gr": "#333333",
    "centos": "#a34e8d",
    "debian": "#D60A51",
    "deepin": "#33c5ff",
    "devuan": "#0f5399",
    "endeavouros": "#7f3fbf",
    "fedora": "#3c6eb4",
    "gentoo": "#61538D",
    "mageia": "#262f45",
    "manjaro": "#34be5b",
    "mint": "#69b63d",
    "nixos-db": "#5073bd",
    "nixos-lb": "#7eb9e3",
    "nord0": "#2e3440",
    "nord1": "#3b4252",
    "nord10": "#5e81ac",
    "nord11": "#bf616a",
    "nord12": "#d08770",
    "nord13": "#ebcb8b",
    "nord14": "#a3be8c",
    "nord15": "#b48ead",
    "nord2": "#434c5e",
    "nord3": "#4c566a",
    "nord4": "#d8dee9",
    "nord5": "#e5e9f0",
    "nord6": "#eceff4",
    "nord7": "#8fbcbb",
    "nord8": "#88c0d0",
    "nord9": "#81a1c1",
    "opensuse": "#74bb20",
    "opensuse-tu": "#2a9489",
    "raspbian": "#cd2355",
    "rocky": "#07ba82",
    "ubuntu": "#dd4814",
    "windows": "#0078ca"
  },
  "console_title_template": "{{.UserName}}@{{.HostName}} [ {{.PWD}} ]",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u256d\u2500",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{if eq .Code 0}}p:nord10{{else}}p:nord11{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "redhat": "\udb84\udd1b"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": "{{.Icon}} ",
          "foreground": "p:nord0",
          "background": "p:nord6",
          "type": "os",
          "style": "diamond",
          "foreground_templates": [
            "{{if contains \"alpine\" .OS}}p:alpine{{end}}",
            "{{if contains \"arch\" .OS}}p:arch-bl{{end}}",
            "{{if contains \"centos\" .OS}}p:nord6{{end}}",
            "{{if contains \"debian\" .OS}}p:debian{{end}}",
            "{{if contains \"deepin\" .OS}}p:deepin{{end}}",
            "{{if contains \"devuan\" .OS}}p:devuan{{end}}",
            "{{if contains \"endeavouros\" .OS}}p:endeavouros{{end}}",
            "{{if contains \"gentoo\" .OS}}p:gentoo{{end}}",
            "{{if contains \"mageia\" .OS}}white{{end}}",
            "{{if contains \"manjaro\" .OS}}p:manjaro{{end}}",
            "{{if contains \"mint\" .OS}}p:mint{{end}}",
            "{{if contains \"nixos\" .OS}}p:nixos-db{{end}}",
            "{{if contains \"opensuse\" .OS}}p:opensuse{{end}}",
            "{{if contains \"opensuse-tumbleweed\" .OS}}p:nord6{{end}}",
            "{{if contains \"raspbian\" .OS}}white{{end}}",
            "{{if contains \"redhat\" .OS}}#ee0000{{end}}",
            "{{if contains \"rocky\" .OS}}white{{end}}",
            "{{if contains \"slackware\" .OS}}white{{end}}",
            "{{if contains \"ubuntu\" .OS}}white{{end}}"
          ],
          "background_templates": [
            "{{if contains \"alma\" .OS}}p:alma{{end}}",
            "{{if contains \"almalinux\" .OS}}p:alma{{end}}",
            "{{if contains \"almalinux9\" .OS}}p:alma{{end}}",
            "{{if contains \"arch\" .OS}}p:arch-gr{{end}}",
            "{{if contains \"centos\" .OS}}p:centos{{end}}",
            "{{if contains \"endeavouros\" .OS}}p:nord0{{end}}",
            "{{if contains \"fedora\" .OS}}p:fedora{{end}}",
            "{{if contains \"macos\" .OS}}black{{end}}",
            "{{if contains \"mageia\" .OS}}p:mageia{{end}}",
            "{{if contains \"nixos\" .OS}}p:nixos-lb{{end}}",
            "{{if contains \"opensuse\" .OS}}#333333{{end}}",
            "{{if contains \"opensuse-tumbleweed\" .OS}}p:nopensuse-tu{{end}}",
            "{{if contains \"raspbian\" .OS}}p:raspbian{{end}}",
            "{{if contains \"redhat\" .OS}}black{{end}}",
            "{{if contains \"rocky\" .OS}}p:rocky{{end}}",
            "{{if contains \"slackware\" .OS}}black{{end}}",
            "{{if contains \"ubuntu\" .OS}}p:ubuntu{{end}}",
            "{{if contains \"windows\" .OS}}p:windows{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{if .SSHSession}}\ueba9 {{end}}{{.UserName}}@{{.HostName}} ",
          "foreground": "p:nord5",
          "powerline_symbol": "\ue0b0",
          "background": "p:nord10",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<parentBackground>\ue0b6</> \uf292 ",
          "foreground": "p:nord6",
          "background": "p:nord11",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\udb81\udf70",
            "folder_separator_icon": "\uf054",
            "folder_separator_template": " \ue0b1 ",
            "home_icon": "\udb80\udedc",
            "style": "agnoster"
          },
          "template": " \udb86\uddee {{ .Path}} ",
          "foreground": "p:nord1",
          "powerline_symbol": "\ue0b0",
          "background": "p:nord8",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "foreground": "p:nord0",
          "powerline_symbol": "\ue0b0",
          "background": "p:nord14",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "type": "executiontime",
          "style": "diamond",
          "template": " \ueaf8 {{ .FormattedMs }}\u2800",
          "foreground": "p:nord0",
          "background": "p:nord13",
          "trailing_diamond": "\ue0b4"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#3C873A",
          "background": "#303030",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "extensions": [
              "*.py",
              "*.ipynb",
              "pyproject.toml",
              "venv.bak"
            ],
            "home_enabled": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#FFE873",
          "background": "#306998",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#ffffff",
          "background": "#88c0d0",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#0d6da8",
          "background": "#0e0e0e",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#06aad5",
          "background": "#ffffff",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#925837",
          "background": "#f3f0ec",
          "type": "rust",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#055b9c",
          "background": "#e1e8e9",
          "type": "dart",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#ce092f",
          "background": "#ffffff",
          "type": "angular",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}",
          "foreground": "#ffffff",
          "background": "#1e293b",
          "type": "nx",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#359a25",
          "background": "#945bb3",
          "type": "julia",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#9c1006",
          "background": "#ffffff",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#5398c2",
          "background": "#ffffff",
          "type": "azfunc",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "#000000",
          "background": "#FF9900",
          "type": "aws",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "foreground": "#ffffff",
          "background": "#316ce4",
          "type": "kubectl",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\uebca <#ffffff>bash</>",
              "cmd": "\uebc4",
              "fish": "\udb80\ude3a<#42a5dc> fish</>",
              "pwsh": "\udb82\ude0a pwsh",
              "zsh": "\udb86\ude02 <#000000>zsh</>"
            }
          },
          "leading_diamond": "\ue0b6",
          "template": "{{.Name}} ",
          "foreground": "p:nord0",
          "background": "p:nord5",
          "type": "shell",
          "style": "diamond",
          "foreground_templates": [
            "{{if contains \"bash\" .Shell}}#4ca825{{end}}",
            "{{if contains \"cmd\" .Shell}}p:nord5{{end}}",
            "{{if contains \"fish\" .Shell}}#49b848{{end}}",
            "{{if contains \"pwsh\" .Shell}}#ffffff{{end}}",
            "{{if contains \"zsh\" .Shell}}#f15a24{{end}}"
          ],
          "background_templates": [
            "{{if contains \"bash\" .Shell}}#283037{{end}}",
            "{{if contains \"cmd\" .Shell}}p:nord0{{end}}",
            "{{if contains \"fish\" .Shell}}#000000{{end}}",
            "{{if contains \"pwsh\" .Shell}}#0374b7{{end}}",
            "{{if contains \"zsh\" .Shell}}#FFFFFF{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
          "foreground": "p:nord0",
          "leading_diamond": "\ue0b2",
          "background": "p:nord8",
          "type": "battery",
          "style": "diamond",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:nord9{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:nord13{{end}}",
            "{{if eq \"Full\" .State.String}}p:nord14{{end}}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "02-Jan"
          },
          "leading_diamond": "\ue0b2",
          "template": " \udb83\ude17 {{ .CurrentDate | date .Format }} ",
          "foreground": "p:nord0",
          "background": "p:nord12",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " \udb85\udc4e {{ .CurrentDate | date .Format }}",
          "foreground": "p:nord0",
          "background": "p:nord11",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{if eq .Code 0}}\u2570\u2500<p:nord11>\ueab6</><p:nord12>\ueab6</><p:nord13>\ueab6</><p:nord14>\ueab6</><p:nord15>\ueab6 {{else}}<p:nord11>\u2570\u2500\ueab6\ueab6\ueab6\ueab6\ueab6 </>{{end}}",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{if eq .Code 0}}p:nord10{{else}}p:nord11{{end}}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
